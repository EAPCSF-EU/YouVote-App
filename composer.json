{
  "name": "youvote/app",
  "description": "YouVote is an open source web-based application that enables users to create ratings and voting contests, and view the results in real time",
  "keywords": [
    "voting",
    "voting-app"
  ],
  "homepage": "https://eapcivilsociety.eu",
  "type": "project",
  "license": "GNU General Public License v3.0",
  "support": {
    "source": "https://github.com/EAPCSF-EU/YouVote-App"
  },
  "minimum-stability": "stable",
  "require": {
    "php": ">=7.4.0",
    "yiisoft/yii2": "~2.0.49",
    "yiisoft/yii2-bootstrap4": "~2.0.0",
    "yiisoft/yii2-swiftmailer": "~2.1.0",
    "dmstr/yii2-adminlte-asset": "^3.0.0-beta1",
    "kartik-v/yii2-widget-datepicker": "@dev",
    "kartik-v/yii2-checkbox-x": "@dev",
    "kartik-v/yii2-widget-datetimepicker": "*",
    "budyaga/yii2-cropper": "*",
    "mailgun/mailgun-php": "^2.8",
    "kriswallsmith/buzz": "^1.0",
    "nyholm/psr7": "^1.1",
    "kartik-v/yii2-icons": "@dev"
  },
  "require-dev": {
    "yiisoft/yii2-debug": "~2.0.0",
    "yiisoft/yii2-gii": "~2.2.5",
    "yiisoft/yii2-faker": "~2.0.0",
    "codeception/verify": "~1.0.0"
  },
  "config": {
    "process-timeout": 1800,
    "fxp-asset": {
      "enabled": false
    },
    "allow-plugins": {
      "yiisoft/yii2-composer": true,
      "php-http/discovery": false
    }
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://asset-packagist.org"
    }
  ]
}
